      <trans-unit id="confirmWorkflowCancellation" datatype="html">
        <source>
      Are you sure you want to cancel &quot;<x id="INTERPOLATION" equiv-text="{{ cancellationRequest.datasetId! }}"/>
      <x id="INTERPOLATION_1" equiv-text="{{ cancellationRequest.datasetName! }}"/>&quot;?
    </source>
        <context-group purpose="location">
          <context context-type="sourcefile">app/app.component.html</context>
          <context context-type="linenumber">9</context>
        </context-group>
        <note priority="1" from="description">show modal to get user confirmation</note>
        <note priority="1" from="meaning">confirm workflow cancellation</note>
      </trans-unit>
      <trans-unit id="welcomeMessage" datatype="html">
